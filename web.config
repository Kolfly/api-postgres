<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="api/index.js" verb="*" modules="iisnode"/>
    </handlers>
    <rewrite>
      <rules>
        <rule name="NodeAPI">
          <match url=".*" />
          <action type="Rewrite" url="api/index.js" />
        </rule>
      </rules>
    </rewrite>
    <iisnode loggingEnabled="true" devErrorsEnabled="true" />
  </system.webServer>
</configuration>